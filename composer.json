      "syruppay_jose_jwa_enc": "syruppay/syruppay/jose/jwa/enc/",
      "syruppay_jose_jwa": "syruppay/syruppay/jose/jwa/",
      "syruppay_jose_jwe": "syruppay/syruppay/jose/jwe/",
      "syruppay_jose_jws": "syruppay/syruppay/jose/jws/",
      "syruppay_jose_util": "syruppay/syruppay/jose/util/",
      "syruppay_jose": "syruppay/syruppay/jose/"
