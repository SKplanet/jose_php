    "syruppay",
    "syrup",
    "JOSE",
    "A128KW",
    "HS256",
    "A128CBC-HS256",
    "JWE",
    "JWS"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "ByeongChan Park",
      "email": "byeongchan.park@sk.com"
    }
  ],
  "require": {
    "php": ">=5.2.*",
    "phpseclib/phpseclib": "0.3.10"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.23",
    "phpdocumentor/phpdocumentor": "2.*"
